{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "indent": [true, "tabs"],
		"ban-types": false,
		"disable-next-line:space-before-function-paren": false
    }
}
